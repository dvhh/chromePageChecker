{
	"manifest_version": 2,
	"content_scripts": [
	],
	"converted_from_user_script": true,
	"description": "Page checker",
	"name":        "Page checker",
	"icons": {
		"48":"Target-Keywords-Icon-48.png",
		"128":"Target-Keywords-Icon-128.png",
		"256":"Target-Keywords-Icon-256.png"
	},
	"options_page": "options.html",
	 "browser_action": {
		"default_icon": {
			"48":"Target-Keywords-Icon-48.png",
			"128":"Target-Keywords-Icon-128.png",
			"256":"Target-Keywords-Icon-256.png"
		},
		"default_title" : "Page checker",
		"default_popup" : "popup.html"
	},
	"background": {
		"scripts":[
			"jquery-3.2.1.min.js",
			"injectCheck.js",
			"background.js"
		]
	},
	"web_accessible_resources": [
	],
	"permissions": [
		"storage",
		"notifications",
		"activeTab",
		"background",
		"tabs",
		"http://*/*",
		"https://*/*"
	],
	"version": "1.0.7"
}
